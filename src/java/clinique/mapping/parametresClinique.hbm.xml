<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="clinique.mapping">
 <class name="ParametresClinique" table="tbl_parametres_clinique">
 <id name="parametreId" column="parametre_id">
 <generator class="assigned"/>
 </id>


<property name="parametre" column="parametre" />
<property name="valeur" column="valeur" />

<property name="operateur" column="operateur" />
<property name="statut" column="statut" />
<property name="dateDebut" column="date_debut" />
<property name="dateFin" column="date_fin" />


 </class>
</hibernate-mapping>
