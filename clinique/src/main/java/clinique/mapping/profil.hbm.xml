<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="clinique.mapping">
 <class name="Profil" table="tbl_profil">
 <id name="profilId" column="profil_id">
 <generator class="assigned"/>
 </id>

<property name="nomProfil" column="nom_profil" />
<property name="statut" column="statut" />
<property name="operateur" column="operateur" />

 </class>
</hibernate-mapping>
