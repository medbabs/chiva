<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="clinique.mapping">
 <class name="Fournisseur" table="tbl_fournisseur">
 <id name="fournisseurId" column="fournisseur_id">
 <generator class="assigned"/>
 </id>

<property name="nomFournisseur" column="nom_fournisseur" />
<property name="adresse" column="adresse" />
<property name="telephone1" column="telephone1" />
<property name="telephone2" column="telephone2" />
<property name="statut" column="statut" />
<property name="operateur" column="operateur" />


 </class>
</hibernate-mapping>
